{
  "name": "encode-cli-lint",
  "version": "0.0.17",
  "description": "前端编码规范工程化脚手架",
  "bin": "./lib/cli.js",
  "main": "./lib/index.js",
  "scripts": {
    "dev": "npm run copyfiles && tsc -w",
    "build": "rm -rf lib && npm run copyfiles && tsc",
    "copyfiles": "copyfiles -a -u 1 \"src/config/**\" lib",
    "test": "npm run build && jest",
    "coverage": "nyc jest --silent --forceExit",
    "encode-spec-clilint-scan": "encode-spec-clilint scan",
    "encode-spec-clilint-fix": "encode-spec-clilint fix"
  },
  "files": [
    "lib/"
  ],
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0"
  },
  "keywords": [
    "encode",
    "lint",
    "cli"
  ],
  "devDependencies": {
    "@types/command-exists": "^1.2.0",
    "@types/cross-spawn": "^6.0.2",
    "@types/ejs": "^3.1.0",
    "@types/eslint": "^8.4.1",
    "@types/fs-extra": "^9.0.13",
    "@types/glob": "^7.2.0",
    "@types/lodash": "^4.14.180",
    "@types/node": "^14.14.11",
    "@types/prettier": "^2.4.4",
    "@types/stylelint": "^14.0.0",
    "@types/text-table": "^0.2.2",
    "copyfiles": "^2.4.1",
    "encode-cli-lint": "^0.0.17",
    "encode-commitlint-config": "^0.0.17",
    "encode-eslint-config": "^0.0.17",
    "encode-markdownlint-config": "^0.0.17",
    "encode-stylelint-config": "^0.0.17",
    "jest": "^27.5.1",
    "nyc": "^15.1.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "chalk": "^4.1.0",
    "command-exists": "^1.2.9",
    "commander": "^6.2.1",
    "cross-spawn": "^7.0.3",
    "ejs": "^3.1.5",
    "execa": "^5.0.0",
    "fast-glob": "^3.2.11",
    "fs-extra": "^9.0.1",
    "glob": "^7.1.6",
    "inquirer": "^7.3.3",
    "is-docker": "^2.1.1",
    "lodash": "^4.17.20",
    "ora": "^5.1.0",
    "strip-ansi": "^6.0.0",
    "stylelint": "^14.3.0",
    "stylelint-config-encode": "^1.0.9",
    "stylelint-scss": "^4.1.0",
    "terminal-link": "^2.1.1",
    "text-table": "^0.2.0",
    "vue-eslint-parser": "^7.3.0"
  },
  "author": "chenghuai",
  "homepage": "https://github.com/qingFeng818/encode-spec-engine#readme",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qingFeng818/encode-spec-engine.git"
  },
  "bugs": {
    "url": "https://github.com/qingFeng818/encode-spec-engine/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "encode-cli-lint commit-file-scan",
      "commit-msg": "encode-cli-lint commit-msg-scan"
    }
  }
}
